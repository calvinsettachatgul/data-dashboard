{
   "name": "DataDashboard",
   "description": "A data-driven dashboard console for report widgets",
   "version": "0.0.1",
   "license": "MIT",
   "main": "web-target/dist/index.html",
   "repository": "github:dnajs/data-dashboard",
   "homepage": "https://github.com/dnajs/data-dashboard",
   "bugs": "https://github.com/dnajs/data-dashboard/issues",
   "keywords": [
      "charts",
      "dashboard",
      "dnajs",
      "html5",
      "javascript",
      "report",
      "widgets"
   ],
   "eslintConfig": {
      "extends": "eslint:recommended",
      "env": {
         "es6": true,
         "browser": true,
         "node": true,
         "mocha": true,
         "jquery": true
      },
      "globals": {
         "dataDashboard": false,
         "dna": false,
         "library": false
      },
      "rules": {
         "no-console": "off",
         "quotes": [
            "warn",
            "single"
         ],
         "semi": [
            "warn",
            "always"
         ]
      }
   },
   "scripts": {
      "web-server": "http-server -p 6868 &",
      "lint": "eslint gulpfile.js src/**/*.js spec/*.js",
      "build": "gulp clean && gulp build && gulp minify && gulp resourcify",
      "pretest": "npm run lint && npm run build",
      "test": "mocha spec/spec.js"
   },
   "dependencies": {
      "chart.js": "~2.7",
      "dna.js": "~1.4",
      "fetch-json": "~2.1",
      "jquery": "~3.3",
      "moment": "~2.22",
      "node-fetch": "~2.3",
      "selectize": "~0.12",
      "simple-datatables": "~1.7",
      "vanilla-datatables": "~1.6",
      "web-ignition": "~1.0",
      "whatwg-fetch": "~3.0"
   },
   "devDependencies": {
      "@babel/core": "~7.1",
      "@babel/preset-env": "~7.1",
      "@fortawesome/fontawesome-free": "~5.5",
      "ajv": "~6.5",
      "cssnano": "~4.1",
      "del": "~3.0",
      "eslint": "~5.9",
      "express": "~4.16",
      "gulp": "~4.0",
      "gulp-babel": "~8.0",
      "gulp-babel-minify": "~0.5",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~2.2",
      "gulp-less": "~4.0",
      "gulp-postcss": "~8.0",
      "gulp-rev-all": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~1.4",
      "http": "~0.0",
      "http-server": "~0.11",
      "jsdom": "~13.0",
      "merge-stream": "~1.0",
      "mocha": "~5.2",
      "postcss": "~7.0",
      "postcss-font-magician": "~2.2",
      "postcss-preset-env": "~6.4"
   }
}
