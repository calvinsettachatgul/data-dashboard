{
   "name": "DataDashboard",
   "description": "A data-driven dashboard console for report widgets",
   "version": "0.0.1",
   "license": "MIT",
   "repository": "dnajs/data-dashboard",
   "homepage": "https://github.com/dnajs/data-dashboard",
   "bugs": "https://github.com/dnajs/data-dashboard/issues",
   "keywords": [
      "charts",
      "dashboard",
      "dnajs",
      "html5",
      "javascript",
      "report",
      "widgets"
   ],
   "eslintConfig": {
      "extends": "eslint:recommended",
      "env": {
         "es6": true,
         "browser": true,
         "node": true,
         "mocha": true,
         "jquery": true
      },
      "globals": {
         "dataDashboard": false,
         "dna": false
      },
      "rules": {
         "no-console": "off",
         "quotes": [
            "warn",
            "single"
         ],
         "semi": [
            "warn",
            "always"
         ]
      }
   },
   "scripts": {
      "lint": "eslint gulpfile.js src/**/*.js spec/*.js",
      "build": "gulp clean && gulp build && gulp minify && gulp resourcify",
      "web-server": "http-server -p 7777 web-target/dist &",
      "pretest": "npm run lint && npm run build && npm run web-server",
      "test": "mocha spec/spec.js",
      "posttest": "pkill -f http-server"
   },
   "dependencies": {
      "chart.js": "~2.7",
      "dna.js": "~1.4",
      "jquery": "~3.3",
      "moment": "~2.22",
      "selectize": "~0.12",
      "vanilla-datatables": "~1.6"
   },
   "devDependencies": {
      "ajv": "~6.5",
      "babel-core": "~6.26",
      "babel-preset-env": "~1.7",
      "cssnano": "~4.0",
      "del": "~3.0",
      "eslint": "~5.2",
      "gulp": "~4.0",
      "gulp-babel": "~7.0",
      "gulp-babel-minify": "~0.4",
      "gulp-concat": "~2.6",
      "gulp-file-include": "~2.0",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~2.1",
      "gulp-less": "~4.0",
      "gulp-postcss": "~7.0",
      "gulp-rev-all": "~0.9",
      "gulp-size": "~3.0",
      "gulp-w3cjs": "~1.3",
      "http-server": "~0.11",
      "jsdom": "~11.12",
      "jshint": "~2.9",
      "merge-stream": "~1.0",
      "mocha": "~5.2",
      "postcss": "~7.0",
      "postcss-cssnext": "~3.1",
      "postcss-font-magician": "~2.2"
   }
}
